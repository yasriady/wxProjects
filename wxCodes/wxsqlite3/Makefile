install_wxsqlite3_lib: $(__wxsqlite3_lib___depname)
	$(INSTALL_DIR) $(DESTDIR)$(libdir)
	$(INSTALL_DATA) $(srcdir)/lib/$(LIBPREFIX)wxcode_$(WX_PORT)$(WXLIBPOSTFIX)_wxsqlite3-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR)$(LIBEXT) $(DESTDIR)$(libdir)

uninstall_wxsqlite3_lib: 
	rm -f $(DESTDIR)$(libdir)/$(LIBPREFIX)wxcode_$(WX_PORT)$(WXLIBPOSTFIX)_wxsqlite3-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR)$(LIBEXT)

install_wxsqlite3_lib_headers: 
	$(INSTALL_DIR) $(DESTDIR)$(prefix)
	for f in $(WXSQLITE3_LIB_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL_DIR) $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL_DATA) $(srcdir)/$$f $(DESTDIR)$(prefix)/$$f; \
	done

uninstall_wxsqlite3_lib_headers: 
	for f in $(WXSQLITE3_LIB_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done

